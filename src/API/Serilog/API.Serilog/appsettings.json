{
  "Papertrail": {
    "host": "logs6.papertrailapp.com",
    "port": 10752
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "restrictedToMinimumLevel": "Information",
          "path": "c:\\Logs\\log_api.serilog.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "API.Serilog",
        "Uri": "http://localhost:60388/healthz"
      }
    ]
  },
  "AllowedHosts": "*"
}
